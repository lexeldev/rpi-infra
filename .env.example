# LocalStack basic configuration
LOCALSTACK_AUTH_TOKEN=
LOCALSTACK_SERVICES=s3,sqs
AWS_REGION=eu-central-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# AWS endpoint for LocalStack (used by collector/processor)
AWS_SQS_ENDPOINT=http://localstack:4566

# TVOC data pipeline configuration
TVOC_SQS_QUEUE_NAME=tvoc-events
TVOC_SQS_QUEUE_URL=http://localstack:4566/000000000000/tvoc-events

# GPIO configuration for the collector (can be "mock" for development)
GPIOZERO_PIN_FACTORY=mock

# Telegram alerts
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Alert thresholds
TVOC_ALERT_THRESHOLD=
CO2_ALERT_THRESHOLD=

# InfluxDB 2.x configuration
INFLUXDB_ENABLED=
INFLUXDB_PORT=8086
INFLUXDB_USERNAME=
INFLUXDB_PASSWORD=
INFLUXDB_ORG=
INFLUXDB_BUCKET=
INFLUXDB_ADMIN_TOKEN=
INFLUXDB_URL=http://influxdb:8086

# Grafana configuration
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=
GRAFANA_ADMIN_PASSWORD=